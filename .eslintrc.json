{
    "env": {
        "browser": true,
        "node":true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
		"no-var": "error",
		"no-unused-vars": ["error", { "args": "none" }],
		"eqeqeq": "error",
		"no-console": "off"
    }
}
